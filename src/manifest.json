{
  // Required
  "manifest_version": 2,
  "name": "Toggle Location",
  "version": "0.1",

  // Recommended
  "description": "change URL",
  "icons": {"128": "icon.png"},

 "permissions": [
    "tabs"
  ],

  // Pick one (or none)
  "browser_action": {
    "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
}