{
  // Required
  "manifest_version": 2,
  "name": "Toggle Location",
  "options_page": "options.html",
  // "options_ui":{
  //     "page":"options.html",
  //     "chrome_style": false
  //   },
  "version": "0.1",

  "description": "change URL",
  "icons": {"128": "icon.png"},

 "permissions": [
    "tabs","storage"
  ],

  // Pick one (or none)
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
    }
}