{
  "manifest_version": 2,
  "name": "Toggle Location",
  "options_page": "options.html",
  "version": "0.2",
  "description": "change URL",
  "icons": {"128": "icon.png"},
  "permissions": [
  "tabs","storage"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}